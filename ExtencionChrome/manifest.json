{
  "manifest_version": 3,
  "name": "Login ISC",
  "version": "1.0",
  "description": "Modificar Login TrakCare",

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_title": "Click to open panel"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  }, 
  "host_permissions": [
    "*://*.intersystemschile.cl/","http://localhost:52773/*"
  ],
  "permissions": ["sidePanel","tabs","activeTab","scripting","tabCapture", "offscreen","cookies"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
}
